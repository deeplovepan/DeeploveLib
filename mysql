mysql

install:
1.download "MySQL Community Server" 
  http://dev.mysql.com/downloads/
2.set path: edit /etc/paths
  add /usr/local/mysql/bin
3.install mysql GUI: Sequel Pro
  

start mysql server:
mysqld_safe

command:
1. connect database:
ex:
connect test

2. show tables;

3. show table field
ex:
describe DataTable;

4. show data in table
ex:
select * from DataTable;

5. delete all data in table
ex:
delete * from DataTable;

