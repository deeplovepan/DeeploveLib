c++:
ex:
#include <vector>
using std::vector;
vector<int> myVector;
myVector.push_back(1);
myVector.push_back(2);
for (vector<int>::iterator it = myVector.begin(); it!=myVector.end(); ++it) 
{
	printf("%d\n", *it);
}

